<!-- Begin page heading -->
<h1 class="page-heading"><%= @model_name.pluralize %> <small>Manage <%= @model_name.pluralize.downcase %></small></h1>
<!-- End page heading -->

<!-- Begin breadcrumb -->
<ol class="breadcrumb default square rsaquo sm">
	<li><a href="/admin"><i class="fa fa-home"></i></a></li>
	<li class="active"><%= @model_name.pluralize %></li>
</ol>
<!-- End breadcrumb -->

<a class="btn btn-info btn-perspective" href="<%%= new_admin_<%= @singular %>_path %>"><i class="fa fa-plus"></i> New <%= @singular %></a>

<!-- Begin static table -->
<h4 class="small-title" style="margin-top: 30px"><%= @model_name.pluralize.upcase %></h4>

<div class="the-box">
	<div class="table-responsive">
		<table class="table table-striped table-hover datatable" id="<%= @model_name.pluralize.underscore %>-datatable">
			<thead class="the-box dark full">
				<tr>
          <th style="width:200px">Actions</th>
          <th style="width:50px">ID</th>
          <% @keys.each do |key| %>
          <th><%= key %></th>
          <% end -%>
        </tr>
			</thead>
			<tbody id="<%= @model_name.pluralize.underscore %>_data">
        <%% @<%= @collection %>.each do |<%= @singular %>| %>
          <tr id="<%= @model_name.pluralize.underscore %>_<%%= <%= @singular %>.id %>">
            <td>
              <a class="btn btn-info" href="<%%= edit_admin_<%= @singular %>_path(<%= @singular %>) %>"><i class="fa fa-pencil"></i> Edit</a>
              <a class="btn btn-danger delete-record" data-id="<%%= <%= @singular %>.id %>" data-type="<%= @model_name.pluralize.underscore %>"><i class="fa fa-trash-o"></i> Delete</a>
            </td>
            <td><%%= <%= @singular %>.id %></td>
            <% @keys.each do |key| %>
            <td><%%= <%= @singular %>.<%= key %> %></td>
            <% end %>
          </tr>
        <%% end %>
			</tbody>
		</table>
	</div><!-- /.table-responsive -->
</div><!-- /.the-box -->
<!-- End static table -->